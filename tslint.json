{
  "$schema": "http://json.schemastore.org/tslint",
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": false,
    "quotemark": [
      true,
      "double"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      
    ]
  }
}
