{
  "$schema": "http://json.schemastore.org/package",
  "name": "@n4bb12/dotfiles",
  "version": "0.0.0-development",
  "private": true,
  "description": "Config files, scripts, productivity stuff",
  "keywords": [
    "aliases",
    "automation",
    "config",
    "dotfiles",
    "productivity",
    "scripts"
  ],
  "homepage": "https://github.com/n4bb12/dotfiles/blob/master/README.md",
  "bugs": "https://github.com/n4bb12/dotfiles/issues",
  "repository": "github:n4bb12/dotfiles",
  "license": "ISC",
  "author": "Abraham Schilling",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bash dot build",
    "format": "prettier --write .",
    "update": "bash dot update",
    "keymap": "ts-node src/karabiner/build.ts",
    "keymap:watch": "nodemon --watch src/karabiner --ext ts --exec yarn keymap"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/graceful-fs": "^4.1.5",
    "@types/node": "^15.14.9",
    "chalk": "^4.1.2",
    "graceful-fs": "^4.2.8",
    "nehemiah": "^1.0.8",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "ts-node": "^9.1.1",
    "type-fest": "^2.12.0",
    "typescript": "^4.4.4"
  }
}
